  "compilerOptions": {
    "rootDir": ".",
    "moduleResolution": "node",
    "jsx": "react-jsx",
    "target": "es2015",
    "outDir": "./dist/",
    "strict": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}